{
  "name": "youpaidforwhat-front",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "font-awesome": "^4.7.0",
    "mdbootstrap": "^4.4.3",
    "mdbreact": "^4.0.0",
    "npm-run-all": "^4.1.2",
    "react": "^16.2.0",
    "react-dom": "^16.1.1",
    "react-redux": "^5.0.6",
    "react-scripts-ts": "2.8.0",
    "redux": "^3.7.2"
  },
  "scripts": {
    "start-js": "react-scripts-ts start",
    "start": "./node_modules/.bin/npm-run-all -p watch-css start-js",
    "build-js": "react-scripts-ts build",
    "build": "./node_modules/.bin/npm-run-all build-css build-js",
    "test": "react-scripts-ts test --env=jsdom",
    "eject": "react-scripts-ts eject",
    "build-css": "node-sass-chokidar src/ -o src/",
    "watch-css": "npm run build-css && node-sass-chokidar src/ -o src/ --watch --recursive"
  },
  "devDependencies": {
    "@types/jest": "^21.1.8",
    "@types/node": "^8.0.53",
    "@types/react": "^16.0.25",
    "@types/react-dom": "^16.0.3",
    "@types/react-redux": "^5.0.14",
    "@types/redux-devtools": "^3.0.42",
    "@types/redux-devtools-dock-monitor": "^1.1.31",
    "@types/redux-devtools-log-monitor": "^1.0.32",
    "node-sass-chokidar": "0.0.3",
    "redux-devtools": "^3.4.1",
    "redux-devtools-dock-monitor": "^1.1.2",
    "redux-devtools-log-monitor": "^1.4.0"
  }
}
